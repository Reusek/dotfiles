#!/bin/sh
# dmoji - dmenu based emoji picker

# GNU cut doesn't grok UTF-8.
EMOJI=$(sed '1,/^exit/d' $0 | dmenu -i -l 10 -fn 'Noto Color Emoji'| grep -Po '^[^ ]*')
if [ "$EMOJI" ]; then
	printf %s "$EMOJI" | xclip -selection clipboard
	exit $?
fi
# made with: curl -sL https://raw.githubusercontent.com/milesj/emojibase/master/packages/data/en/raw.json | jq -r '.[] | .emoji + "  " + .name + "  " + (.tags | map("#" + .) | join(" "))'
exit 1
